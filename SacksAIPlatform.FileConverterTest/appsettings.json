{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore": "Warning"
    },
    "RedirectToDebug": true,
    "EnableConsoleLogging": false
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=SacksAIPlatform;Trusted_Connection=true;MultipleActiveResultSets=true"
  },
  "TestConfiguration": {
    "CreateSampleFiles": true,
    "TestDataPath": "test-data.csv",
    "BackupConfigPath": "config-backups",
    "RunAllTests": true
  },
  "ParserDefaults": {
    "EnableConsoleDiagnostics": true,
    "ShowDetailedParsing": true,
    "SaveIntermediateResults": true
  },
  "DatabaseOptions": {
    "AutoCreateDatabase": true,
    "AutoSeedData": true,
    "ConnectionTimeout": 30
  }
}